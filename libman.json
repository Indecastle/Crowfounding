{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/jquery/"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@4.4.1",
      "destination": "wwwroot/bootstrap/",
      "files": [
        "dist/css/bootstrap.min.css",
        "dist/js/bootstrap.min.js",
        "dist/js/bootstrap.bundle.min.js"
      ]
    },
    {
      "library": "font-awesome@5.11.2",
      "destination": "wwwroot/font-awesome/"
    },
    {
      "library": "font-awesome-animation@0.2.1",
      "destination": "wwwroot/font-awesome-animation/"
    }
  ]
}